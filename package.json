{
  "name": "ghost-terminal",
  "version": "1.0.0",
  "description": "KYC.RIP Tactical Desktop Terminal",
  "main": "./out/main/index.js",
  "author": "KYC.RIP",
  "scripts": {
    "copy-worker": "cp node_modules/monero-ts/dist/monero.worker.js public/",
    "dev": "npm run copy-worker && electron-vite dev",
    "build": "npm run copy-worker && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:renderer": "tsc --noEmit -p tsconfig.renderer.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:renderer",
    "start": "electron-vite preview"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "electron-store": "^11.0.2",
    "ethers": "6.16.0",
    "framer-motion": "12.34.0",
    "http-proxy": "^1.18.1",
    "js-yaml": "^4.1.1",
    "lightweight-charts": "^5.1.0",
    "lucide-react": "0.564.0",
    "monero-ts": "0.11.8",
    "process": "^0.11.10",
    "qrcode.react": "^4.2.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-helmet-async": "2.0.5",
    "react-router-dom": "7.13.0",
    "socks-proxy-agent": "^8.0.5",
    "tar": "^7.5.9"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@tailwindcss/vite": "4.1.18",
    "@types/http-proxy": "^1.17.17",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "25.2.3",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@types/tar": "^6.1.13",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "10.4.24",
    "electron": "40.4.1",
    "electron-builder": "^24.13.3",
    "electron-vite": "5.0.0",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-node-polyfills": "^0.25.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron"
    ],
    "ignoredBuiltDependencies": [
      "esbuild"
    ]
  }
}
